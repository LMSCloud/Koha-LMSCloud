{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "__": "readonly",
    "dayjs": "readonly",
    "$": "readonly",
    "jQuery": "readonly"
  },
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"]
  }
}


