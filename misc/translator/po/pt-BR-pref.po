msgid ""
"accounting.pref#AutoCreditNumber# Automatically generate credit numbers in "
"the form 1, 2, 3"
msgstr "Gera automaticamente números de crédito no formato 1, 2, 3"

# Accounting > Features
msgid ""
"accounting.pref#AutoCreditNumber# Automatically generate credit numbers in "
"the form <branchcode>yyyymm0001"
msgstr ""
"Gera automaticamente números de crédito no formato <branchcode> aaaam0001"

